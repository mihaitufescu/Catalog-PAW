@page
@model CatalogOnline.Pages.UsersModel
@{
    ViewData["Title"] = "Users";
}

<h1>User List</h1>

<table class="table">
    <thead>
        <tr>
            <th>Username</th>
            <th>Legal Name</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model.Users)
        {
            <tr>
                <td>@user.username</td>
                <td>@user.legal_name</td>
                <td>@user.email</td>
            </tr>
        }
    </tbody>
</table>